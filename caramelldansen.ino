#include <Adafruit_NeoPixel.h>

int tonePin = 8;

uint32_t colors[] = { 0xff0000, 0x00ff00, 0x0000ff, 0xFF8500, 0xFF28FF, 0xFFF000, 0x00E7FF };

int cnt = 0;


Adafruit_NeoPixel strip2 = Adafruit_NeoPixel(1, 12, NEO_GRB + NEO_KHZ800);

void noteAndColor(uint8_t pin, uint32_t frequency, float duration) {
  for (uint16_t i = 0; i < strip2.numPixels(); i++) {
    strip2.setPixelColor(i, colors[cnt]);
    strip2.show();
  }
  tone(pin, frequency, duration);
  ++cnt;
  if (cnt > 6) cnt = 0;
}

void setup() {
  strip2.begin();
  strip2.setBrightness(80);
  strip2.show();
}

